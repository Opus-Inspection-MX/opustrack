# Database
DATABASE_URL="postgresql://user:password@localhost:5432/opustrack?schema=public"

# NextAuth
NEXTAUTH_SECRET="your-secret-here-generate-with-openssl-rand-base64-32"
NEXTAUTH_URL="http://localhost:3000"

# File Storage
# Options: "vercel-blob" (default) | "filesystem"
FILE_STORAGE_PROVIDER="vercel-blob"

# Vercel Blob Storage (required if FILE_STORAGE_PROVIDER="vercel-blob")
# Get this token from: https://vercel.com/dashboard/stores
BLOB_READ_WRITE_TOKEN="vercel_blob_rw_xxxxxxxxxxxxxxxx"
